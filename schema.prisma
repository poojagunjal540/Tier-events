generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          String   @id @default(cuid())
  title       String
  description String
  imageUrl    String
  date        DateTime
  tier        String
  createdBy   String
  createdAt   DateTime @default(now())
}
